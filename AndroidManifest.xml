<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Eiyeron.tpandroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.Eiyeron.tpandroid.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.Eiyeron.tpandroid.AjoutContact"
            android:label="@string/title_activity_ajout_contact" >
        </activity>
        <activity
            android:name="com.Eiyeron.tpandroid.ListContact"
            android:label="@string/title_activity_list_contact" >
        </activity>
        <activity
            android:name="com.Eiyeron.tpandroid.ContactDetail"
            android:label="@string/title_activity_contact_detail" >
        </activity>
    </application>

</manifest>
